generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Product {
  id       Int   @id @default(autoincrement())
  product  String @db.VarChar(255)
  quantity Int
  price    Float
  provide  String  @db.VarChar(255)
  date     DateTime @default(now())

  @@map("product")
}
