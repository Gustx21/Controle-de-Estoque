generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Product {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  product  String
  quantity Int
  price    Float
  provide  String   @unique
  date     DateTime @default(now())
}
